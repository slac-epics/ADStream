# dataStream.substitutions

# stdArray Plugin
file NDStdArrays.template
{
    {
    PORT            = "\$(IMAGE_NAME)",
    P               = "\$(CAM):",
    R               = "$(PORT):",
    ADDR            = "\$(ADDR=0)",
    TIMEOUT         = "\$(TIMEOUT=1)",
    TSE             = "\$(TSE=-2)",
    ENABLED         = "\$(ENABLED=1)"
    NDARRAY_PORT    = "\$(CAM_PORT)",
    NDARRAY_ADDR    = "\$(NDARRAY_ADDR=0)",
    TYPE            = "\$(IMAGE_TYPE)",
    FTVL            = "\$(IMAGE_FTVL)",
    NELEMENTS       = "\$(STREAM_NELM=\$(IMAGE_NELM))",
    }
}

# PVA Plugin
file NDPva.template
{
    {
    PORT            = "\$(IMAGE_NAME):Pva",
    P               = "\$(CAM):",
    R               = "$(PORT):",
    ADDR            = "\$(ADDR=0)",
    ENABLED         = "\$(ENABLED=1)",
    TIMEOUT         = "\$(TIMEOUT=1)",
    NDARRAY_PORT    = "\$(CAM_PORT)",
    NDARRAY_ADDR    = "\$(NDARRAY_ADDR=0)",
    RATE_SMOOTH     = "\$(RATE_SMOOTH=0.0)",
    TSE             = "\$(TSE=-2)",
    }
}

# ROI Plugin
file NDROI.template
{
    {
    PORT            = "\$(IMAGE_NAME):ROI",
    P               = "\$(CAM):",
    R               = "$(PORT):",
    ADDR            = "\$(ADDR=0)",
    TIMEOUT         = "\$(TIMEOUT=1)",
    TSE             = "\$(TSE=-2)",
    ENABLED         = "\$(ENABLED=0)"
    PLUGIN_SRC      = "\$(CAM_PORT)",
    NDARRAY_PORT    = "\$(CAM_PORT)",
    NDARRAY_ADDR    = "\$(NDARRAY_ADDR=0)",
    BIN_X           = "\$(BIN_X=1)",
    BIN_Y           = "\$(BIN_Y=1)",
    ENABLE_X        = "\$(ENABLE_X=0)",
    ENABLE_Y        = "\$(ENABLE_Y=0)",
    ENABLE_SCALE    = "\$(ENABLE_SCALE=0)",
    SCALE           = "\$(SCALE=1)",
    DATA_TYPE_OUT   = 1,
    }
}

